// Test sock_bind with invalid socket handle
// Should return -1 for invalid handle

// Get platform-appropriate shell
var shell = "";
if (os() == "win") {
    shell = "cmd.exe";
} else if (os() == "linux") {
    shell = "/bin/sh";
}

var result = sock_bind(-1, shell);
print("Result with invalid handle:", result);
if (result == -1) { print("PASS"); }

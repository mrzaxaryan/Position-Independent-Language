// Test WebSocket send and receive

var result = true;
var ws = ws_connect("wss://echo.websocket.org");
var sent = ws_send_text(ws, "Hello");
print("Sent:", sent);
var data = ws_recv(ws);
print("Received:", len(data), "bytes");
if (len(data) > 0) { print("PASS"); }
else {
    print("FAIL");
    result = false;
}
ws_close(ws);

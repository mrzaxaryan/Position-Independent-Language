// Test sock_redirect with empty process path
// Should return -1 for empty path

var result = true;
var sock = sock_connect("httpbin.org", 80);
if (sock >= 0) {
    result = sock_redirect(sock, "");
    print("Result with empty path:", result);
    if (result == -1) { print("PASS"); }
    else {print("FAIL"); result = false; }
    sock_close(sock);
} else {
    // If we can't connect, skip the test
    print("FAIL");
    result = false;
}
